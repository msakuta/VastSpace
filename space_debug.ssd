	new coordsys Lagrange1 el1{
		cs_radius 200
		object1 Earth
		object2 moon/Moon
		updirection 0 1 
		fullname "Earth Lagrange 1"
		teleport
		bookmark "Earth Lagrange 1" 0 0 10
		bookmark "Earth Lagrange 1,2" 0 0 -10 "Quatd.rotation(PI, Vec3d(0,1,0))"
		warp
		rstation 0 -25
#		addent scarry 5 -25 0
#		addent scontainer 4 -24 0
